Set rand_series = For(1,3,Serie(Real k)
{
  Serie ser = SubSer(Gaussian(0,1,Weekly),y2015m04,y2015m12d31);
  Eval("RS_"<<FormatReal(k,"%05.0lf")+"=ser")
