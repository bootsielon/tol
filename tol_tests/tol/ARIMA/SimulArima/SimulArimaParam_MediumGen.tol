//////////////////////////////////////////////////////////////////////////////
// FILE    : SimulArimaParam_MediumGen.tol
// PURPOSE : Parámetros del simulador de modelos ARIMA por generación 
//           aleatoria.
//////////////////////////////////////////////////////////////////////////////


//////////////////////////////////////////////////////////////////////////////
// Opciones de simulación 
// Por simplicidad sólo se simulan modelos ARIMA con dos factores, uno regular
// y otro estacional, aunque se podría extender a varias estacionalidades.
//////////////////////////////////////////////////////////////////////////////

Set SimulationParameters =
[[
  Real Sigma            =   0.1; //Desviación típica de los residuos
  Real MinCycles        =     4;   //Mínimo de ciclos estacionales del output
  Real MinData          =   500;   //Mínima longitud de la serie output 
  Real MaxData          =  2000;   //Máxima longitud de la serie output 
  Real MaxInterruptions =    20;   //Máximo nº de interrupciones
  Real MaxInputs        =    50;   //Máximo nº de inputs
  Real MaxRegDIF        =     0;   //Máximo nº de diferencias regulares
  Real MaxSeaDIF        =     0;   //Máximo nº de diferencias estacionales
  Real MaxRegAR         =     3;   //Máximo grado AR regular
  Real MaxRegMA         =     3;   //Máximo grado AR estacional
  Real MaxSeaAR         =     1;   //Máximo grado MA regular
  Real MaxSeaMA         =     1;   //Máximo grado MA estacional
  Real RestrictLTqp     = True;
//Posibles estacionalidades 
  Set  PeriodsTable     = [[ 7, 12, 24]]
]];

