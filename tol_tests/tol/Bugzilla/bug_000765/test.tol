  Serie residuos = SubSer(Gaussian(0,0.2)+Pulse(y2004),y2000,y2008);

  Serie transformaTimeSet(Serie residuos) {

    TimeSet nuevoTimeSet = Diario;
//  Serie DatCh(residuos,Diario,FirstS)
    Serie DatCh(residuos,nuevoTimeSet,FirstS)
    

