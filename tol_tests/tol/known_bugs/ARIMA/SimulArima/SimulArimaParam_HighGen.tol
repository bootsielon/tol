//////////////////////////////////////////////////////////////////////////////
// FILE    : SimulArimaParam_HighGen.tol
// PURPOSE : Parámetros del simulador de modelos ARIMA por generación 
//           aleatoria.
//////////////////////////////////////////////////////////////////////////////


//////////////////////////////////////////////////////////////////////////////
// Opciones de simulación 
// Por simplicidad sólo se simulan modelos ARIMA con dos factores, uno regular
// y otro estacional, aunque se podría extender a varias estacionalidades.
//////////////////////////////////////////////////////////////////////////////

Set SimulationParameters =
[[
  Real Sigma            =   0.1;   //Desviación típica de los residuos
  Real MinCycles        =     3;   //Mínimo de ciclos estacionales del output
  Real MinData          =  2000;   //Mínima longitud de la serie output 
  Real MaxData          = 10000;   //Máxima longitud de la serie output 
  Real MaxInterruptions =   100;   //Máximo nº de interrupciones
  Real MaxInputs        =   500;   //Máximo nº de inputs
  Real MaxRegDIF        =     0;   //Máximo nº de diferencias regulares
  Real MaxSeaDIF        =     0;   //Máximo nº de diferencias estacionales
  Real MaxRegAR         =     4;   //Máximo grado AR regular
  Real MaxRegMA         =     4;   //Máximo grado AR estacional
  Real MaxSeaAR         =     2;   //Máximo grado MA regular
  Real MaxSeaMA         =     2;   //Máximo grado MA estacional
  Real RestrictLTqp     = True;
//Posibles estacionalidades 
  Set  PeriodsTable     = [[ 7, 12, 24, 52, 168, 364 ]]
]];

