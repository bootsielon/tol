// FILE   : functions.tol
// PURPOSE: Generic functions to handle with AIA features
//////////////////////////////////////////////////////////////////////////////

//////////////////////////////////////////////////////////////////////////////
  Text Aia.OutlierInfo.GetExpression(@OutlierInfo out)
//////////////////////////////////////////////////////////////////////////////
{
  "+("+FormatReal(out->value_,"%.15lg")+")*"+
   out->name_+"("<<
   out->date_+","+
   out->dating_+")"
};

//////////////////////////////////////////////////////////////////////////////
  Serie Aia.OutlierInfo.GetSerie(@OutlierInfo out)
//////////////////////////////////////////////////////////////////////////////
{
  Eval(Aia.OutlierInfo.GetExpression(out))
};


