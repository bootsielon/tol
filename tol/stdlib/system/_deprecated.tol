//////////////////////////////////////////////////////////////////////////////
  Real SystemCat(Text path)
//////////////////////////////////////////////////////////////////////////////
{
  System(CatSys + ReplaceSlash(path))
};


//////////////////////////////////////////////////////////////////////////////
  Real SystemDelete(Text path)
//////////////////////////////////////////////////////////////////////////////
{
  //System(DeleteSys + ReplaceSlash(path))
  FileDelete(path)
};


//////////////////////////////////////////////////////////////////////////////
  Real SystemCopy(Text path)
//////////////////////////////////////////////////////////////////////////////
{
  System(CopySys + ReplaceSlash(path))
};

